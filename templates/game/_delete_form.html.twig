{% extends 'base-back.html.twig' %}
<form method="post" action="{{ path('app_game_delete', {'gameId': game.gameId}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ game.gameId) }}">
    <button class="btn">Delete</button>
</form>
